[ req ]
default_bits = 2048
prompt = no
default_md = sha256
distinguished_name = dn
req_extensions = req_ext
x509_extensions = v3_req

[ dn ]
C = RO
ST = IASI
L = IASI
O = TRAVELMENOW
OU = IT
CN = travelmenow

[ req_ext ]
subjectAltName = @alt_names

[ v3_req ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = travelmenow.online
